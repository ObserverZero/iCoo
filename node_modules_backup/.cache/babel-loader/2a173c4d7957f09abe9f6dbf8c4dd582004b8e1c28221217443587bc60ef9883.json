icText = ref('')\n    let topicBanner = ref('')\n    let name = ref('')\n    let topic = ref('')\n    let content = reactive({\n      content: 'chat'\n    })\n    address = \"!\" + this.$route.params.id + \":matrix.org\"\n    return {\n      room,\n      content,\n      address,\n      children,\n      topicText,\n      topicBanner,\n    }\n  },\n  components: {\n    ChatItem,\n    GroupSpace,\n    BoardSpace,\n    ChatSpace,\n    IonItem,\n    IonContent,\n    IonCardHeader,\n    IonCardSubtitle,\n    IonCardContent,\n    IonList,\n    IonButton,\n    IonButtons,\n    IonCardTitle,\n    IonAvatar,\n    IonToggle,\n    IonLabel,\n    IonSegment,\n    IonSegmentButton,\n    IonCard,\n    IonIcon,\n    IonFab,\n    IonFabButton,\n    modalController,\n    IonFabList,\n    IonPage,\n    IonHeader,\n    IonFooter,\n    IonImg,\n  },\n  props: {\n    name: String,\n    groupId: String,\n    topic: Object,\n  },\n  methods: {\n    async openModal() {\n      const modal = await modalController.create({\n        component: ChatModal,\n        componentProps: {\n          groupId: this.groupId,\n        }\n      });\n      crossPage.storeModalData({yo: \"wow this\"});\n      return await modal.present();\n    },\n    segmentChanged(event) {\n      this.content.content = event.detail.value;\n    },\n    async fetchData() {\n      const id = \"!\" + this.$route.params.id + \":matrix.org\";\n      setInterval(async () => {\n        try {\n        this.room = await client.getRoomById(id)\n        this.topicText = this.room.topic.text\n        this.topicBanner = \"@/assets/banners/factory.jpg\"\n        console.log(this.topicBanner)\n        } catch {\n          return\n        }\n        children = client.getGroupContent(address)\n      }, 1000);\n    },\n    async getMessages() {\n      const id = \"!\" + this.$route.params.groupId + \":matrix.org\";\n      let room = await client.getRoomById(id)\n\n      //console.log(room)\n    },\n  },\n  created() {\n    this.$watch(\n     