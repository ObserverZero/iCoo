},\n    async sendText() {\n      let returned = await client.sendText(this.content.id, this.text)\n      content = await client.getChatContent(this.$props.message.id)\n      return\n    },\n    async fetchData() {\n        content = await client.getChatContent(this.$props.message.id)\n      },\n    sendAndFetch() {\n      this.sendText()\n      this.fetchData()\n    }\n  },\n  created() {\n    this.$watch(\n        () => this.$route.params,\n        () => {\n          this.fetchData()\n    content = this.$props.message\n        },\n        // fetch the data when the view is created and the data is\n        // already being observed\n        {immediate: true}\n    )\n  },\n});\n</script>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}