of mountains\" src=\"../../public/assets/banners/factory_semi_b&W.png\"/>\n      <IonCardHeader>\n        <IonCardTitle>{{ name }}</IonCardTitle>\n        <IonCardSubtitle>&{{ $route.params.id }}</IonCardSubtitle>\n        <IonCardContent>{{ topic }}</IonCardContent>\n      </IonCardHeader>\n\n      <IonSegment value=\"chat\" :scrollable=\"true\">\n        <IonSegmentButton value=\"chat\">\n          <IonIcon :icon=\"chatbox\" color=\"tertiary\"/>\n        </IonSegmentButton>\n        <IonSegmentButton value=\"board\">\n          <IonIcon :icon=\"clipboard\" color=\"tertiary\"/>\n        </IonSegmentButton>\n        <IonSegmentButton value=\"groups\">\n          <IonIcon :icon=\"colorFilterSharp\" color=\"tertiary\"/>\n        </IonSegmentButton>\n      </IonSegment>\n\n    </IonCard>\n\n    <div v-if=\"content.content === 'chat'\">\n      <ChatSpace/>\n    </div>\n    <div v-if-else=\"content.content === 'board'\">\n      <BoardSpace/>\n    </div>\n    <div v-if-else=\"content.content === 'groups'\">\n      <GroupSpace/>\n    </div>\n\n    <IonFab slot=\"fixed\" vertical=\"bottom\" horizontal=\"end\">\n      <IonFabButton color=\"dark\">\n        <IonIcon :icon=\"add\"></IonIcon>\n      </IonFabButton>\n      <IonFabList side=\"top\">\n        <IonFabButton @click=\"openModal\">\n          <IonIcon :icon=\"colorFilterSharp\" color=\"tertiary\"/>\n        </IonFabButton>\n        <IonFabButton>\n          <IonIcon :icon=\"clipboard\" color=\"tertiary\"></IonIcon>\n        </IonFabButton>\n        <IonFabButton>\n          <IonIcon :icon=\"chatbox\" color=\"tertiary\"></IonIcon>\n        </IonFabButton>\n      </IonFabList>\n    </IonFab>\n\n\n  </IonPage>\n</template>\n\n<script>\n/* eslint-disable vue/no-unused-components */\nimport {\n  IonItem,\n  IonCardHeader,\n  IonCardSubtitle,\n  IonCardContent,\n  IonList,\n  IonButton,\n  IonButtons,\n  IonCardTitle,\n  IonAvatar,\n  IonToggle,\n  IonLabel,\n  IonSegment,\n  IonSegmentButton,\n  IonCard,\n  IonIcon,\n  IonFab,\n  IonFabButton,\n  modalController,\n  IonFabList,\n} from '@ionic/vue';\nimport {add, addCircle, chatbox, clipboard, colorFilterSharp,} from 'ionicons/icons';\nimport {defineComponent, reactive} from 'vue';\nimport {useMatrixClient} from \"@/stores/MatrixClient\";\nimport Modal from '@/menus/Modal.vue';\nimport ChatSpace from \"@/components/blehChatSpace.vue\";\nimport BoardSpace from \"@/components/BoardSpace.vue\";\nimport GroupSpace from \"@/components/GroupSpace.vue\";\n\nlet client = useMatrixClient();\n\nlet content = reactive({\n  content: 'chat',\n});\n\nexport default {\n  name: \"GroupDisplay\",\n  setup() {\n    return {\n      chatbox,\n      clipboard,\n      colorFilterSharp,\n      addCircle,\n      add,\n    }\n  },\n  components: {\n    GroupSpace,\n    BoardSpace,\n    ChatSpace,\n    IonItem,\n    IonCardHeader,\n    IonCardSubtitle,\n    IonCardContent,\n    IonList,\n    IonButton,\n    IonButtons,\n    IonCardTitle,\n    IonAvatar,\n    IonToggle,\n    IonLabel,\n    IonSegment,\n    IonSegmentButton,\n    IonCard,\n    IonIcon,\n    IonFab,\n    IonFabButton,\n    modalController,\n    IonFabList,\n  },\n  props: {\n    name: String,\n    groupId: String,\n    topic: String,\n  },\n  methods: {\n    async openModal() {\n      const modal = await modalController.create({\n        component: Modal,\n        componentProps: {\n          groupId: this.groupId,\n        }\n      });\n      return await modal.present();\n    },\n  },\n  data() {\n    return {\n      content,\n    }\n  },\n}\n</script>\n\n<style scoped>\n\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}