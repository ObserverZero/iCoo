n  data() {\n    let joinedSearchable = false;\n    let profile = client.getOwnProfile();\n    return {\n      profile,\n      joinedSearchable,\n    }\n  },\n  setup() {\n    return {\n      ellipse,\n      calendarClear,\n      peopleCircle,\n      personCircle,\n      beer,\n      search,\n      notifications,\n      addCircle,\n      colorFilterSharp,\n      logIn,\n      chatbox,\n      cog,\n      logOut,\n    }\n  },\n});\n</script>\n<style scoped>\n.i {\n  position: relative;\n  bottom: 3.5px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}